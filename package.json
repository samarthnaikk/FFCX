{
  "name": "ffcx-timetable",
  "version": "1.0.0",
  "description": "Advanced FFCS Timetable Optimizer for VIT Students",
  "main": "server/server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "start": "cd server && npm start"
  },
  "keywords": [
    "timetable",
    "ffcs",
    "vit",
    "mern",
    "react",
    "nodejs",
    "express"
  ],
  "author": "Samarth Naik",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samarthnaikk/FFCX"
  }
}